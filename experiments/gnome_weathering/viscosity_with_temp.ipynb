   "source": [
    "T = np.linspace(273, 303, 10)  # environmentallly realistic range of temps.\n",
    "kv1 = kvisc(T, a1)\n",
    "kv2 = kvisc(T, a2)\n",
    "\n",
    "plt.plot(T, kv1)\n",
    "plt.plot(T, kv2, '.')\n",
    "plt.plot(t1, v1, 'o')\n",
    "plt.plot(t2, v2, 'o')\n"
   ]
  },
